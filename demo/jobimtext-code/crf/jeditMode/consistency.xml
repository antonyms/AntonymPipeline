<?xml version="1.0"?>
<!DOCTYPE MODE SYSTEM "xmode.dtd">

<MODE>

<PROPS>
	<PROPERTY NAME="lineComment" VALUE="//" />
</PROPS>

<RULES>	
	<EOL_SPAN TYPE="COMMENT1">//</EOL_SPAN>
	<SEQ TYPE="KEYWORD1">DEF&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">PROPOSE&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">SUBPROPOSE&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">DECLARE&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">MAKECONST&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">TEST&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">OBJECTIVE&gt;</SEQ>
	<SEQ TYPE="KEYWORD1">:</SEQ>
	<SEQ TYPE="KEYWORD1">RANDOM:</SEQ>
	<SEQ TYPE="KEYWORD1">WEIGHTEDMAX:</SEQ>
	<SEQ TYPE="KEYWORD1">FORALL:</SEQ>
	<SEQ TYPE="KEYWORD1">^</SEQ>
	<SEQ TYPE="KEYWORD1">@</SEQ>
	<!--<SEQ TYPE="KEYWORD1">/\</SEQ>
	<SEQ TYPE="KEYWORD1">\/</SEQ>-->
	
	
	<SPAN TYPE="KEYWORD2" NO_LINE_BREAK="TRUE">
		<BEGIN>T_{</BEGIN>
		<END>}</END>
	</SPAN>
	<SPAN TYPE="KEYWORD2" NO_LINE_BREAK="TRUE">
		<BEGIN>B_{</BEGIN>
		<END>}</END>
	</SPAN>
	
	<SEQ TYPE="OPERATOR">+</SEQ>
	<SEQ TYPE="OPERATOR">*</SEQ>
	<SEQ TYPE="OPERATOR">-</SEQ>
	<SEQ TYPE="OPERATOR">{</SEQ>
	<SEQ TYPE="OPERATOR">}</SEQ>
	<SEQ TYPE="OPERATOR">(</SEQ>
	<SEQ TYPE="OPERATOR">)</SEQ>
	<SEQ TYPE="OPERATOR">[</SEQ>
	<SEQ TYPE="OPERATOR">]</SEQ>
	<SEQ TYPE="OPERATOR">!</SEQ>
	<SEQ TYPE="OPERATOR">&lt;</SEQ>
	<SEQ TYPE="OPERATOR">&lt;=</SEQ>
	<SEQ TYPE="OPERATOR">&gt;</SEQ>
	<SEQ TYPE="OPERATOR">&gt;=</SEQ>
	<SEQ TYPE="OPERATOR">=</SEQ>
	
	<SEQ_REGEXP HASH_CHARS="\" AT_WORD_START="TRUE"
 		TYPE="LITERAL3">\\[A-Za-z0-9]+</SEQ_REGEXP>
 	<SEQ_REGEXP HASH_CHARS="$" AT_WORD_START="TRUE"
 		TYPE="LITERAL4">\$[A-Za-z0-9]+</SEQ_REGEXP>
		
</RULES>

</MODE>

